class Solution {
    public int removeDuplicates(int[] nums) {
        int n = nums.length;
        Map<Integer, Integer> mp = new HashMap<>();
        int k=0;
        for(int i=0; i<n; i++){
            mp.put(nums[i], mp.getOrDefault(nums[i],0)+1);

            if(mp.get(nums[i]) <= 2){
                nums[k] = nums[i];
                k++;
            }
        }
        return k;  
    }
}
