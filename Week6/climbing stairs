class Solution {
    public int climbStairs(int n) {
        int[] memo = new int[n+1];
        Arrays.fill(memo, -1);
        return ways(memo, n);
    }
    public int ways(int[] memo, int n){
        if(n == 0) return 1;
        if(n == -1) return 0;

        if(memo[n] != -1){
            return memo[n];
        }
        memo[n] = ways(memo, n-1)+ways(memo, n-2);
        return memo[n];
    }
}
