class Node{
    String data;
    Node next;
    Node prev;

    Node(String data){
        this.data = data;
        next = null;
        prev = null;
    }
}
class BrowserHistory {
    Node Homepage; 
    Node curr;  
    public BrowserHistory(String homepage) {
        Homepage = new Node(homepage); 
        curr = Homepage;     
    }
    
    
    public void visit(String url) {
        Node newPage = new Node(url);
        curr.next = newPage;
        newPage.prev = curr;  
        curr = newPage;
    }
    
    public String back(int steps) {
        while(steps-- > 0){
            if(curr.prev != null && curr != null){
                curr = curr.prev;
            }
        }
        return curr.data;
    }
    
    public String forward(int steps) {
        while(steps-- > 0){
            if(curr.next != null && curr != null){
                curr = curr.next;
            }
        }
        return curr.data;  
    }
}
